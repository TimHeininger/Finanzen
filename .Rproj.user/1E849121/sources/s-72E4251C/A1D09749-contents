function(input, output, session){
  
#' ------------------------------------------------------------------------------------------------------------------------
#' ------------------------------------------------------------------------------------------------------------------------
# Buchungen erzeugen #### 
#' ------------------------------------------------------------------------------------------------------------------------
#' ------------------------------------------------------------------------------------------------------------------------  
  
  label_buchungen<- reactive({
    label_suche_TH(input$buchung_erstellen_label)
    })
  
  output$buchungen_erzeugen_table_buchungen<-renderTable(label_buchungen,
                                                         striped = FALSE,
                                                         hover = TRUE,
                                                         bordered = FALSE,
                                                         spacing = "s")
  

  
}